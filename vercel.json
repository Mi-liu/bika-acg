{
  "version": 2,
  "buildCommand": "node scripts/vercel-build.js",
  "outputDirectory": "dist",
  "installCommand": "npm install --legacy-peer-deps",
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
